{
  "presets": ["env"],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "plugins": [
    ["transform-class-properties", { "spec": true }],
    "transform-object-rest-spread",
    "add-module-exports",
    ["transform-runtime", { "regenerator": true }]
  ]
}
